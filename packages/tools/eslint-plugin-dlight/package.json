{
  "name": "eslint-plugin-dlight",
  "version": "1.0.0",
  "description": "DLight eslint plugin",
  "author": {
    "name": "IanDx",
    "email": "iandxssxx@gmail.com"
  },
  "keywords": [
    "dlight.js"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --sourcemap",
    "typecheck": "tsc --noEmit"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "clean": true,
    "dts": true,
    "minify": true
  }
}